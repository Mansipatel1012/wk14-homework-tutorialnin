    <test name="Test Suite">
        <classes>
            <class name="tutorialsninja.com.testsuite.AccountLoginPageTest"></class>
            <class name="tutorialsninja.com.testsuite.TopMenuTest"></class>
            <class name="tutorialsninja.com.testsuite.DesktopPageTest"></class>
            <class name="tutorialsninja.com.testsuite.LaptopAndNoteBookPageTest"></class>
            <class name="tutorialsninja.com.testsuite.AccountRegisterPageTest"></class>
        </classes>
    </test>
